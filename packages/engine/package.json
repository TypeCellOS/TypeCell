{
  "name": "@typecell-org/engine",
  "version": "0.0.3",
  "private": true,
  "dependencies": {
    "es-module-shims": "1.4.3",
    "lodash": "^4.17.21",
    "mobx": "^6.2.0",
    "react": "^18.2.0",
    "vscode-lib": "^0.1.2",
    "@typecell-org/shared": "*"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "mocha": "^10.2.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^10.0.1",
    "@types/lodash": "^4.14.182",
    "@types/react": "^18.0.25",
    "rimraf": "^3.0.2",
    "typescript": "5.0.4",
    "@playwright/test": "^1.33.0",
    "playwright-test": "^12.1.1",
    "vitest": "^0.33.0",
    "jsdom": "^22.1.0",
    "@vitest/coverage-v8": "^0.33.0"
  },
  "source": "src/index.ts",
  "types": "types/src/index.d.ts",
  "main": "dist/src/index.js",
  "type": "module",
  "scripts": {
